{
	"name": "ClienteExcel_a_clienteSql",
	"properties": {
		"description": "cargar archivo cliente desde esxcel hacia sql",
		"activities": [
			{
				"name": "Copy cliente hacia sql",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "ExcelSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						}
					},
					"sink": {
						"type": "AzureSqlSink",
						"writeBehavior": "insert",
						"sqlWriterUseTableLock": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "adls_Cliente_origen",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "asql_DB_repaso_adf",
						"type": "DatasetReference"
					}
				]
			}
		],
		"folder": {
			"name": "Move and Transform Activity"
		},
		"annotations": []
	}
}